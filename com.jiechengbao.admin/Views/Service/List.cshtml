
@{
    ViewBag.Title = "List";
    Layout = "~/Views/LayoutPage.cshtml";
}
@using com.jiechengbao.admin.Models
<div class="container">
    <label>服务销售状况</label>
    <table class="table table-striped table-hover table-bordered">
        <thead>
            <tr>
                <th>用户</th>
                <th>服务名</th>
                <th>剩余服务次数</th>
                <th>服务总次数</th>
                <th>服务购买时间</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in ViewData["MyServiceModelList"] as IEnumerable<MyServiceModel>)
            {
                <tr>
                    <td><img src="@item.MemberImage" />@item.MemberName</td>
                    <td>@item.ServiceName</td>
                    <td>@item.CurrentCount</td>
                    <td>@item.TotalCount</td>
                    <td>@item.CreateTime</td>
                </tr>
            }
        </tbody>
    </table>
</div>