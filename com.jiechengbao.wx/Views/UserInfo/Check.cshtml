
@{
    ViewBag.Title = "Check";
    Layout = "~/Views/LayoutPage2.cshtml";
}
<style>
    .Adv {
        padding: 10px 0px 10px 0px;
        height: 100px;
    }

    #NewsContent {
        float: left;
        width: 700px;
        border: #CCC solid 1px;
        margin-left: 10px;
        background-color: #FFF;
    }

    #NewsHeader {
        background: url(http://www.cx580.com/Web/images/headerbg.jpg) no-repeat;
        height: 19px;
        line-height: 19px;
        padding: 15px;
        font-size: 18px;
        font-family: 微软雅黑;
        color: #FFF;
    }

    .Icon_TopNews {
        float: left;
        width: 30px;
        height: 22px;
        background: url(http://www.cx580.com/Web/images/icon.png) no-repeat;
        background-position: -48px -15px;
        margin-right: 10px;
    }

    .header {
        height: 80px;
        padding: 20px;
        color: #333;
    }

        .header h3 {
            font-size: 24px;
            font-family: 微软雅黑;
            font-weight: normal;
            margin-bottom: 20px;
        }

    .NewsInfo {
        display: block;
        clear: both;
        width: 100%;
        margin: auto;
    }

    .publisher, .releasetime {
        float: left;
        padding-right: 20px;
    }

    .Content {
        padding: 20px;
    }

        .Content a {
            color: #178CE6;
        }

            .Content a:hover {
                color: #178CE6;
                text-decoration: underline;
            }
    #Paltform {
        float: left;
    }

    .HotLine {
        border: #d1d1d1 solid 1px;
        height: 143px;
        width: 335px;
        margin-top: 10px;
        background: url(http://www.cx580.com/Web/images/bg_143.png) repeat-x;
        text-align: center;
    }

        .HotLine img {
            margin: 15px auto;
        }

        .HotLine label {
            display: block;
            clear: both;
            width: 275px;
            margin: auto;
            border-top: #CCC dashed 1px;
            line-height: 32px;
        }

    .fx_div {
        height: 37px;
        width: 662px;
        border-bottom: 1px dashed #B4B4B4;
        border-top: 1px dashed #B4B4B4;
        padding: 20px 0px 10px 0px;
        vertical-align: middle;
        line-height: 20px;
        margin: auto;
    }

    .fxzt {
        float: left;
        margin-right: 4px;
        font-size: 12px;
        font-weight: bold;
    }

    .fxzt2 {
        float: left;
        margin-right: 8px;
        margin-top: 3px;
    }

    .syp {
        line-height: 25px;
        font-size: 14px;
        margin-left: 20px;
    }

        .syp a {
            color: #3754C2;
            font-size: 14px;
        }




    .xyp {
        line-height: 25px;
        font-size: 14px;
        margin-left: 20px;
        margin-bottom: 10px;
    }

        .xyp a {
            color: #3754C2;
            font-size: 14px;
        }

    .xxx_ {
        width: 672px;
        border-bottom: 1px dashed #B4B4B4;
        margin: auto;
    }
</style>
<link type="text/css" rel="stylesheet" href="http://v1.jiathis.com/code/css/jiathis_share.css">
